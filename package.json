{
  "name": "bloodhub",
  "version": "1.0.0",
  "description": "Connecting donors to people in need",
  "main": "app.js",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "nodemailer": "^6.3.1",
    "nodemailer-mailgun-transport": "^1.4.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "cross-env": "^6.0.3",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "test": "jest --testTimeout=10000 --ci",
    "test:watch": "cross-env NODE_ENV=test jest --testTimeout=10000 --watch",
    "start:dev": "nodemon --exec babel-node src/server.js",
    "migrate": "npx sequelize db:migrate && npx sequelize db:seed:all",
    "migrate:reset": "npx sequelize db:migrate:undo:all && npm run migrate",
    "build": "babel src --out-dir dist",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ozimos/BloodHub.git"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ozimos/BloodHub/issues"
  },
  "homepage": "https://github.com/ozimos/BloodHub#readme"
}
